<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ContactDetailFragment"
    >

    <ImageView
        android:id="@+id/iv_detail_back"
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="20dp"
        android:clickable="true"
        android:focusable="true"
        android:elevation="2dp"
        android:src="@drawable/back"
        app:tint="#7E7E7E" />

    <ImageView
        android:id="@+id/iv_detail_edit"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_margin="20dp"
        android:elevation="2dp"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/edit"
        app:tint="#7E7E7E" />

    <ImageView
        android:id="@+id/iv_detail_profile"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        app:layout_constraintTop_toTopOf="parent
"        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="1dp"
        android:src="@drawable/chaewon"
        android:foregroundGravity="center"
        android:background="@drawable/border1"
        />

    <ScrollView
        android:id="@+id/detail_scrollview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_detail_profile"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <Button
                android:id="@+id/btn_detail_message"
                android:layout_width="165dp"
                android:layout_height="60dp"
                android:layout_marginTop="30dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBCACACA"
                android:text="메시지"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_name"
                app:layout_constraintStart_toStartOf="parent" />


            <Button
                android:id="@+id/btn_detail_call"
                android:layout_width="165dp"
                android:layout_height="60dp"
                android:layout_marginTop="30dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#D32BEC2F"
                android:text="전화"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_name"
                app:layout_constraintEnd_toEndOf="parent" />

            <Button
                android:id="@+id/btn_detail_name"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBFFFFFF"
                android:clickable="false"
                android:gravity="left"
                android:padding="10dp"
                android:text="이름"
                android:textColor="#9E9E9E"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_number"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/et_detail_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="김채원"
                android:elevation="2dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_name"
                app:layout_constraintStart_toStartOf="@+id/btn_detail_name"
                />

            <Button
                android:id="@+id/btn_detail_number"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBFFFFFF"
                android:clickable="false"
                android:gravity="left"
                android:padding="10dp"
                android:text="번호"
                android:textColor="#9E9E9E"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_email"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/et_detail_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:elevation="2dp"
                android:text="010-4539-6554"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_number"
                app:layout_constraintStart_toStartOf="@+id/btn_detail_number"
                />

            <Button
                android:id="@+id/btn_detail_email"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBFFFFFF"
                android:clickable="false"
                android:gravity="left"
                android:padding="10dp"
                android:text="이메일"
                android:textColor="#9E9E9E"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/et_detail_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:elevation="2dp"
                android:text="haerin@gmail.com"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_email"
                app:layout_constraintStart_toStartOf="@+id/btn_detail_email"
                />

            <Button
                android:id="@+id/btn_detail_sticker"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:elevation="0dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBFFFFFF"
                android:clickable="false"
                android:gravity="left"
                android:padding="10dp"
                android:text="스티커"
                android:textColor="#9E9E9E"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/btn_detail_delete"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/btn_detail_add_sticker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="16sp"
                android:text="추가"
                android:elevation="2dp"
                android:textColor="#9E9E9E"
                android:padding="10dp"
                android:clickable="true"
                android:focusable="true"
                app:layout_constraintTop_toTopOf="@+id/btn_detail_sticker"
                app:layout_constraintEnd_toEndOf="@+id/btn_detail_sticker"
                />

            <ImageView
                android:id="@+id/iv_detail_sticker1_bg"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:elevation="2dp"
                android:src="@drawable/shape_circle_background"
                android:layout_marginStart="20dp"
                android:layout_marginBottom="40dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toStartOf="@+id/btn_detail_sticker"
                app:tint="@color/gray_b" />

            <ImageView
                android:id="@+id/iv_detail_sticker1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:padding="12dp"
                android:elevation="2dp"
                android:src="@drawable/imoge01_thumbsup"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintTop_toTopOf="@+id/iv_detail_sticker1_bg" />

            <TextView
                android:id="@+id/iv_detail_sticker1_count"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:elevation="2dp"
                android:text="3"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:src="@drawable/shape_circle_background"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker1_bg"
                android:background="@drawable/round_border2"
                />

            <ImageView
                android:id="@+id/iv_detail_sticker2_bg"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:elevation="2dp"
                android:src="@drawable/shape_circle_background"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toEndOf="@+id/iv_detail_sticker1_bg"
                app:tint="@color/gray_b" />

            <ImageView
                android:id="@+id/iv_detail_sticker2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:padding="12dp"
                android:elevation="2dp"
                android:src="@drawable/imoge05_partyingface"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintTop_toTopOf="@+id/iv_detail_sticker2_bg" />

            <TextView
                android:id="@+id/iv_detail_sticker2_count"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:elevation="2dp"
                android:text="3"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:src="@drawable/shape_circle_background"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker2_bg"
                android:background="@drawable/round_border2"
                />

            <ImageView
                android:id="@+id/iv_detail_sticker3_bg"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:elevation="2dp"
                android:src="@drawable/shape_circle_background"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toEndOf="@+id/iv_detail_sticker2_bg"
                app:tint="@color/gray_b" />

            <ImageView
                android:id="@+id/iv_detail_sticker3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:padding="12dp"
                android:elevation="2dp"
                android:src="@drawable/imoge14_moneywithwings"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintTop_toTopOf="@+id/iv_detail_sticker3_bg" />

            <TextView
                android:id="@+id/iv_detail_sticker3_count"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:elevation="2dp"
                android:text="3"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:src="@drawable/shape_circle_background"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker3_bg"
                android:background="@drawable/round_border2"
                />

            <ImageView
                android:id="@+id/iv_detail_sticker4_bg"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:elevation="2dp"
                android:src="@drawable/shape_circle_background"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toEndOf="@+id/iv_detail_sticker3_bg"
                app:tint="@color/gray_b" />

            <ImageView
                android:id="@+id/iv_detail_sticker4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:padding="12dp"
                android:elevation="2dp"
                android:src="@drawable/imoge07_confusedface"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintTop_toTopOf="@+id/iv_detail_sticker4_bg" />

            <TextView
                android:id="@+id/iv_detail_sticker4_count"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:elevation="2dp"
                android:text="3"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:src="@drawable/shape_circle_background"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker4_bg"
                android:background="@drawable/round_border2"
                />

            <ImageView
                android:id="@+id/iv_detail_sticker5_bg"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:elevation="2dp"
                android:src="@drawable/shape_circle_background"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/btn_detail_sticker"
                app:layout_constraintStart_toEndOf="@+id/iv_detail_sticker4_bg"
                app:tint="@color/gray_b" />

            <ImageView
                android:id="@+id/iv_detail_sticker5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center"
                android:padding="12dp"
                android:elevation="2dp"
                android:src="@drawable/imoge20_foldedhands"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintTop_toTopOf="@+id/iv_detail_sticker5_bg" />

            <TextView
                android:id="@+id/iv_detail_sticker5_count"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:elevation="2dp"
                android:text="3"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:src="@drawable/shape_circle_background"
                app:layout_constraintBottom_toBottomOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker5_bg"
                android:background="@drawable/round_border2"
                />

            <Button
                android:id="@+id/btn_detail_delete"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="80dp"
                android:background="@drawable/round_border"
                android:backgroundTint="#CBFF0000"
                android:text="삭제"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tv_detail_sticker1_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고마워"
                android:elevation="2dp"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker1_bg"
                app:layout_constraintTop_toBottomOf="@+id/iv_detail_sticker1_bg"
                />

            <TextView
                android:id="@+id/tv_detail_sticker2_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고마워"
                android:elevation="2dp"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker2_bg"
                app:layout_constraintTop_toBottomOf="@+id/iv_detail_sticker2_bg"
                />

            <TextView
                android:id="@+id/tv_detail_sticker3_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고마워"
                android:elevation="2dp"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker3_bg"
                app:layout_constraintTop_toBottomOf="@+id/iv_detail_sticker3_bg"
                />

            <TextView
                android:id="@+id/tv_detail_sticker4_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고마워"
                android:elevation="2dp"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker4_bg"
                app:layout_constraintTop_toBottomOf="@+id/iv_detail_sticker4_bg"
                />

            <TextView
                android:id="@+id/tv_detail_sticker5_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고마워"
                android:elevation="2dp"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                app:layout_constraintStart_toStartOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintEnd_toEndOf="@+id/iv_detail_sticker5_bg"
                app:layout_constraintTop_toBottomOf="@+id/iv_detail_sticker5_bg"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>